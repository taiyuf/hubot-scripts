{
  "name": "hubot-scripts",
  "version": "1.0.0",
  "description": "Hubot scripts.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test",
    "testone": "mocha",
    "build": "(cd ./src && babel -d ../build ./*)",
    "watch": "npm run watch:start",
    "watch:start": "(cd ./src && babel -d ../build ./* --watch &)",
    "watch:stop": "for p in `ps awx|grep 'hubot-scripts/node_modules/.bin/babel'|grep -v 'grep'|awk '{print $1}'`; do kill $p; done"
  },
  "babel": {
    "presets": [
      "es2015",
      "babel-preset-power-assert"
    ],
    "env": {
      "development": {
        "presets": [
          "power-assert"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuf/hubot-scripts.git"
  },
  "keywords": [
    "hubot"
  ],
  "author": "Taiyu Fujii",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taiyuf/hubot-scripts/issues"
  },
  "homepage": "https://github.com/taiyuf/hubot-scripts",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "fs": "0.0.2",
    "generator-hubot": "^0.4.0",
    "js-yaml": "^3.5.2",
    "lodash": "^4.13.1",
    "log4js": "^0.6.29",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "superagent": "^2.1.0",
    "url": "^0.11.0",
    "webpack": "^1.13.1",
    "yo": "^1.8.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "hubot-mock-adapter": "^1.0.0",
    "hubot-mock-adapter-helper": "",
    "mocha": "^2.2.5",
    "nock": "^2.18.2",
    "power-assert": "^1.4.1"
  }
}
